{
  "name": "lmaustralia-org",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "migrate": "cross-env NODE_OPTIONS=--no-deprecation payload migrate",
    "migrate:force": "printf 'y\\n' | cross-env NODE_OPTIONS=--no-deprecation payload migrate",
    "prebuild": "printf 'y\\n' | cross-env NODE_OPTIONS=--no-deprecation payload migrate || true",
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev --turbo",
    "dev:clean": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next node_modules/.cache && next dev --turbo",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test --config=playwright.config.ts",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts"
  },
  "dependencies": {
    "@payloadcms/admin-bar": "3.68.1",
    "@payloadcms/db-postgres": "3.68.1",
    "@payloadcms/db-vercel-postgres": "3.68.1",
    "@payloadcms/email-resend": "3.68.1",
    "@payloadcms/live-preview-react": "3.68.1",
    "@payloadcms/next": "3.68.1",
    "@payloadcms/plugin-form-builder": "3.68.1",
    "@payloadcms/plugin-nested-docs": "3.68.1",
    "@payloadcms/plugin-redirects": "3.68.1",
    "@payloadcms/plugin-search": "3.68.1",
    "@payloadcms/plugin-seo": "3.68.1",
    "@payloadcms/richtext-lexical": "3.68.1",
    "@payloadcms/storage-vercel-blob": "3.68.1",
    "@payloadcms/ui": "3.68.1",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-email/components": "^1.0.1",
    "@react-email/render": "^2.0.0",
    "@sentry/nextjs": "^10.28.0",
    "@vercel/analytics": "^1.6.1",
    "@vercel/blob": "^2.0.0",
    "@vercel/speed-insights": "^1.3.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^10.1.0",
    "d3-geo": "^3.1.1",
    "dotenv": "17.2.3",
    "graphql": "^16.12.0",
    "lucide-react": "^0.555.0",
    "mapbox-gl": "^3.17.0",
    "motion": "^12.23.25",
    "next": "16.0.8",
    "next-auth": "^5.0.0-beta.30",
    "next-sitemap": "^4.2.3",
    "payload": "3.68.1",
    "payload-authjs": "^0.10.0",
    "payload-lexical-typography": "^0.5.2",
    "prism-react-renderer": "^2.4.1",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-google-recaptcha-v3": "^1.11.0",
    "react-hook-form": "7.68.0",
    "sharp": "0.34.5",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@playwright/test": "1.57.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/react": "16.3.0",
    "@turf/turf": "^7.3.1",
    "@types/d3-geo": "^3.1.0",
    "@types/escape-html": "^1.0.4",
    "@types/geojson": "^7946.0.16",
    "@types/node": "24.10.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.1",
    "baseline-browser-mapping": "^2.9.0",
    "eslint": "^9.39.1",
    "eslint-config-next": "^15.1.6",
    "jsdom": "27.2.0",
    "next-devtools-mcp": "^0.3.5",
    "playwright": "1.57.0",
    "playwright-core": "1.57.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "tailwindcss": "^4.1.17",
    "to-ico": "^1.1.5",
    "tsx": "^4.21.0",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "4.0.15"
  },
  "engines": {
    "node": "^18.20.2 || >=22.20.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ],
    "overrides": {
      "baseline-browser-mapping": "latest",
      "@payloadcms/plugin-cloud-storage": "3.68.1"
    }
  },
  "overrides": {
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3"
  }
}
